<%- include('./partials/head') %> 
    <br>
    <div class="container sm-6">
        <h1>Ingrese Producto</h1>
        <form action="/productos" method="post">
            <div class="form-floating mb-3">
                <input name="title" type="text" class="form-control" id="floatingInput" placeholder="firstname">
                <label for="floatingInput">Title</label>
            </div>
            <div class="form-floating mb-3">
                <input name="price" type="text" class="form-control" id="floatingInput" placeholder="Lastname">
                <label for="floatingInput">Price</label>
            </div>
            <div class="form-floating mb-3">
                <input name="file" type="file" class="form-control" id="floatingInput" placeholder="age">
                <label for="floatingInput">Image</label>
            </div>
            <div class="col-mb-3">
                <br>
                <button type="submit" class="btn btn-dark">Enviar</button>
            </div>

        </form>
        <br>

  </div>
</body>
</html>